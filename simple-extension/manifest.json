{
  "manifest_version": 2,
  "name": "Project 101",
  "version": "1.0",
  "description": "Simple extension as possible",
  "browser_action": {
    "default_icon": "./icons/micro.png",
    "default_title": "Project 101",
    "default_popup": "./popup/popup.html"
  },
  "background": {
    "page": "background/background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["*://gogle/*"],
      "js": ["content.js"]
    }
  ]
}
